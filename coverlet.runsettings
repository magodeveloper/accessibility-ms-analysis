<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
  <DataCollectionRunSettings>
    <DataCollectors>
      <DataCollector friendlyName="XPlat code coverage">
        <Configuration>
          <Format>cobertura</Format>
          <ExcludeByFile>
            **/Migrations/**
            **/*ModelSnapshot.cs
            **/InitialCreate.cs
            **/InitialCreate.Designer.cs
            **/ServiceRegistration.cs
            **/AnalysisDbContext.cs
          </ExcludeByFile>
          <ExcludeByAssembly>
            Analysis.Infrastructure
          </ExcludeByAssembly>
          <Exclude>
            [*]*Program
            [*]*Startup
            [Analysis.Infrastructure]*
            [*]*.Migrations.*
          </Exclude>
        </Configuration>
      </DataCollector>
    </DataCollectors>
  </DataCollectionRunSettings>
</RunSettings>